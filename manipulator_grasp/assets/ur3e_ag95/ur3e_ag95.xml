<mujoco model="ur3e_ag95">
  <compiler angle="radian" meshdir="." autolimits="true"/>

  <option integrator="implicitfast" impratio="10"/>

  <default>
    <!-- UR3E默认设置 -->
    <default class="ur3e">
      <material specular="0.5" shininess="0.25"/>
      <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1"/>
      <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="1000" biasprm="0 -1000 -200"/>
      <default class="size2">
        <general forcerange="-54 54"/>
      </default>
      <default class="size1">
        <default class="size1_limited">
          <joint range="-3.1415 3.1415"/>
          <general gainprm="500" biasprm="0 -500 -100" ctrlrange="-3.1415 3.1415" forcerange="-28 28"/>
        </default>
      </default>
      <default class="size0">
        <general gainprm="250" biasprm="0 -250 -50" forcerange="-9 9"/>
      </default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="collision">
        <geom type="capsule" group="3"/>
        <default class="eef_collision">
          <geom type="cylinder"/>
        </default>
      </default>
      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>
    </default>

    <!-- AG95默认设置 -->
    <default class="ag95">
      <joint range="0 0.943"/>
      <general 
        dyntype="none" gaintype="fixed" 
        biastype="affine" dynprm="1 0 0" 
        gainprm="10 0 0" biasprm="0 -10 -0.1"
        forcerange="-5 5" ctrlrange="0 0.943"
      />
      <default class="visual2">
        <geom group="2" type="mesh" contype="0" conaffinity="0" density="0"/>
      </default>
      <default class="collision2">
        <geom group="3" type="mesh" density="2700"/>
        <default class="pad_box_0">
          <geom solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.45 0.45 0.45 1"/>
        </default>
        <default class="pad_box_1">
          <geom solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.55 0.55 0.55 1"/>
        </default>
        <default class="sphere_collision">
          <geom type="sphere" size="0.0006" rgba="1 0 0 1" group="3" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" mass="1e-6"/>
        </default>
      </default>
    </default>
  </default>

  <asset>
    <!-- UR3E材质 -->
    <material class="ur3e" name="urblack" rgba="0.033 0.033 0.033 1"/>
    <material class="ur3e" name="jointgray" rgba="0.278 0.278 0.278 1"/>
    <material class="ur3e" name="linkgray" rgba="0.82 0.82 0.82 1"/>
    <material class="ur3e" name="urblue" rgba="0.49 0.678 0.8 1"/>

    <!-- AG95材质 -->
    <material name="grey" specular="0.5" shininess="0.25" rgba="0.51764705882 0.53764705882 0.54764705882 1"/>
    <material name="black" specular="0.5" shininess="0.25" rgba="0.1 0.1 0.1 1"/>

    <!-- UR3E网格 -->
    <mesh file="../ur3e/assets/base_0.obj"/>
    <mesh file="../ur3e/assets/base_1.obj"/>
    <mesh file="../ur3e/assets/shoulder_0.obj"/>
    <mesh file="../ur3e/assets/shoulder_1.obj"/>
    <mesh file="../ur3e/assets/shoulder_2.obj"/>
    <mesh file="../ur3e/assets/upperarm_0.obj"/>
    <mesh file="../ur3e/assets/upperarm_1.obj"/>
    <mesh file="../ur3e/assets/upperarm_2.obj"/>
    <mesh file="../ur3e/assets/upperarm_3.obj"/>
    <mesh file="../ur3e/assets/forearm_0.obj"/>
    <mesh file="../ur3e/assets/forearm_1.obj"/>
    <mesh file="../ur3e/assets/forearm_2.obj"/>
    <mesh file="../ur3e/assets/forearm_3.obj"/>
    <mesh file="../ur3e/assets/wrist1_0.obj"/>
    <mesh file="../ur3e/assets/wrist1_1.obj"/>
    <mesh file="../ur3e/assets/wrist1_2.obj"/>
    <mesh file="../ur3e/assets/wrist2_0.obj"/>
    <mesh file="../ur3e/assets/wrist2_1.obj"/>
    <mesh file="../ur3e/assets/wrist2_2.obj"/>
    <mesh file="../ur3e/assets/wrist3.obj"/>

    <!-- AG95网格 -->
    <mesh name="base_link" file="../ag95/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    <mesh name="gripper_body" file="../ag95/meshes/gripper_body.stl" scale="0.001 0.001 0.001"/>
    <mesh name="outer_knuckle" file="../ag95/meshes/outer_knuckle.stl" scale="0.001 0.001 0.001"/>
    <mesh name="inner_knuckle" file="../ag95/meshes/inner_knuckle.stl" scale="0.001 0.001 0.001"/>
    <mesh name="finger" file="../ag95/meshes/finger.stl" scale="0.001 0.001 0.001"/>
    <mesh name="finger_pad_0" file="../ag95/meshes/finger_pad_0.stl" scale="1 1 1"/>
    <mesh name="finger_pad_1" file="../ag95/meshes/finger_pad_1.stl" scale="1 1 1"/>
  </asset>

  <worldbody>
    <light name="spotlight" mode="targetbodycom" target="wrist_2_link" pos="0 -1 2"/>
    <body name="ur3e_base" pos="1.0 0.6 0.745" quat="1 0 0 0" childclass="ur3e">
      <inertial mass="2.0" pos="0 0 0" diaginertia="0.0030531654454 0.0030531654454 0.005625"/>
      <geom mesh="base_0" material="urblack" class="visual"/>
      <geom mesh="base_1" material="jointgray" class="visual"/>
      
      <body name="shoulder_link" pos="0 0 0.163">
        <inertial mass="2.0" pos="0 0 0" diaginertia="0.008093163429399999 0.008093163429399999 0.005625"/>
        <joint name="shoulder_pan_joint" class="size2" axis="0 0 1"/>
        <geom mesh="shoulder_0" material="urblue" class="visual"/>
        <geom mesh="shoulder_1" material="urblack" class="visual"/>
        <geom mesh="shoulder_2" material="jointgray" class="visual"/>
        <geom class="collision" size="0.05 0.05" pos="0 0 -0.04"/>
        
        <body name="upper_arm_link" pos="0 0.12 0" quat="1 0 1 0">
          <inertial mass="3.42" pos="0 0 0.121825" diaginertia="0.021728483221103233 0.021728483221103233 0.00961875"/>
          <joint name="shoulder_lift_joint" class="size2"/>
          <geom mesh="upperarm_0" material="urblack" class="visual"/>
          <geom mesh="upperarm_1" material="urblue" class="visual"/>
          <geom mesh="upperarm_2" material="jointgray" class="visual"/>
          <geom mesh="upperarm_3" material="linkgray" class="visual"/>
          <geom class="collision" pos="0 -0.04 0" quat="1 1 0 0" size="0.05 0.05"/>
          <geom class="collision" size="0.04 0.1" pos="0 0 0.1"/>
          
          <body name="forearm_link" pos="0 -0.093 0.24355">
            <inertial mass="1.26" pos="0 0 0.1066" diaginertia="0.0065445675821719194 0.0065445675821719194 0.00354375"/>
            <joint name="elbow_joint" class="size1_limited"/>
            <geom mesh="forearm_0" material="urblue" class="visual"/>
            <geom mesh="forearm_1" material="linkgray" class="visual"/>
            <geom mesh="forearm_2" material="urblack" class="visual"/>
            <geom mesh="forearm_3" material="jointgray" class="visual"/>
            <geom class="collision" pos="0 0.06 0" quat="1 1 0 0" size="0.04 0.052"/>
            <geom class="collision" size="0.03 0.1" pos="0 0 0.1"/>
            
            <body name="wrist_1_link" pos="0 0 0.2132" quat="1 0 1 0">
              <inertial mass="0.8" pos="0 0.104 0" diaginertia="0.002084999166 0.002084999166 0.00225"/>
              <joint name="wrist_1_joint" class="size0"/>
              <geom mesh="wrist1_0" material="urblack" class="visual"/>
              <geom mesh="wrist1_1" material="urblue" class="visual"/>
              <geom mesh="wrist1_2" material="jointgray" class="visual"/>
              <geom class="collision" pos="0 0.03 0" quat="1 1 0 0" size="0.032 0.045"/>
              
              <body name="wrist_2_link" pos="0 0.104 0">
                <inertial mass="0.8" pos="0 0 0.1" diaginertia="0.002084999166 0.002084999166 0.00225"/>
                <joint name="wrist_2_joint" axis="0 0 1" class="size0"/>
                <geom mesh="wrist2_0" material="urblue" class="visual"/>
                <geom mesh="wrist2_1" material="urblack" class="visual"/>
                <geom mesh="wrist2_2" material="jointgray" class="visual"/>
                <geom class="collision" size="0.032 0.045" pos="0 0 0.025"/>
                <geom class="collision" pos="0 0.015 0.085" quat="1 1 0 0" size="0.032 0.035"/>
                
                <body name="wrist_3_link" pos="0 0 0.08535">
                  <inertial mass="0.35" pos="0 0.0771683 0" quat="1 0 0 1"
                    diaginertia="0.00013626661215999998 0.00013626661215999998 0.0001792"/>
                  <joint name="wrist_3_joint" class="size0"/>
                  <geom material="linkgray" mesh="wrist3" class="visual"/>
                  
                  <!-- AG95机械手安装在UR3E末端 -->
                  <body name="ag95_base" pos="0 0.09 0" euler="-1.57 0 0" childclass="ag95">
                    <geom class="collision2" mesh="base_link"/>
                    <geom class="visual2" mesh="base_link" material="grey"/>
                    <geom class="collision2" mesh="gripper_body"/>
                    <geom class="visual2" mesh="gripper_body" material="black"/>

                    <!-- 左侧机械手结构 -->
                    <body name="left_inner_knuckle" pos="0.016 -0.0254 0.10586" quat="0.922844 0 0.385173 0">
                      <joint name="left_inner_knuckle_joint" pos="0 0 0" axis="0 -1 0"/>
                      <geom class="collision2" pos="-0.016 0.0254 -0.10586" mesh="inner_knuckle"/>
                      <geom class="visual2" pos="-0.016 0.0254 -0.10586" mesh="inner_knuckle" material="black"/>
                    </body>
                    
                    <body name="left_outer_knuckle" pos="0.036673 -0.00875 0.098336" quat="0.924908 0 -0.380191 0">
                      <joint name="left_outer_knuckle_joint" pos="0 0 0" axis="0 -1 0"/>
                      <geom class="collision2" pos="-0.036673 0.00875 -0.098336" mesh="outer_knuckle"/>
                      <geom class="visual2" pos="-0.036673 0.00875 -0.098336" mesh="outer_knuckle" material="black"/>
                      
                      <body name="left_finger" pos="0.055 0 0" quat="0.924908 0 0.380191 0">
                        <joint name="left_finger_joint" pos="0 0 0" axis="0 1 0"/>
                        <geom class="collision2" pos="-0.091673 0.00875 -0.098336" mesh="finger"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger" material="black"/>
                        <geom class="pad_box_0" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_0"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_0" material="grey"/>
                        <geom class="pad_box_1" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_1"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_1" material="grey"/>
                        
                        <!-- 接触点 -->
                        <geom pos="-0.028 0 0.02" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.02" class="sphere_collision"/>
                        <geom pos="-0.028 0 0.032" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.032" class="sphere_collision"/>
                        <geom pos="-0.028 0 0.044" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.044" class="sphere_collision"/>
                      </body>
                    </body>

                    <!-- 右侧机械手结构 -->
                    <body name="right_inner_knuckle" pos="-0.016 0.0254 0.10586" quat="0 0.385173 0 -0.922844">
                      <joint name="right_inner_knuckle_joint" pos="0 0 0" axis="0 -1 0"/>
                      <geom class="collision2" pos="-0.016 0.0254 -0.10586" mesh="inner_knuckle"/>
                      <geom class="visual2" pos="-0.016 0.0254 -0.10586" mesh="inner_knuckle" material="black"/>
                    </body>
                    
                    <body name="right_outer_knuckle" pos="-0.036673 0.00875 0.098336" quat="0 0.380191 0 0.924908">
                      <joint name="right_outer_knuckle_joint" pos="0 0 0" axis="0 -1 0"/>
                      <geom class="collision2" pos="-0.036673 0.00875 -0.098336" mesh="outer_knuckle"/>
                      <geom class="visual2" pos="-0.036673 0.00875 -0.098336" mesh="outer_knuckle" material="black"/>
                      
                      <body name="right_finger" pos="0.055 0 0" quat="0.924908 0 0.380191 0">
                        <joint name="right_finger_joint" pos="0 0 0" axis="0 1 0"/>
                        <geom class="collision2" pos="-0.091673 0.00875 -0.098336" mesh="finger"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger" material="black"/>
                        <geom class="pad_box_0" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_0"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_0" material="grey"/>
                        <geom class="pad_box_1" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_1"/>
                        <geom class="visual2" pos="-0.091673 0.00875 -0.098336" mesh="finger_pad_1" material="grey"/>
                        
                        <!-- 接触点 -->
                        <geom pos="-0.028 0 0.02" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.02" class="sphere_collision"/>
                        <geom pos="-0.028 0 0.032" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.032" class="sphere_collision"/>
                        <geom pos="-0.028 0 0.044" class="sphere_collision"/>
                        <geom pos="-0.028 0.0175 0.044" class="sphere_collision"/>
                      </body>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <!-- 碰撞排除配置 -->
  <contact>
    <!-- UR3E内部碰撞排除 -->
    <!-- <exclude body1="wrist_2_link" body2="forearm_link"/> -->
    
    <!-- AG95内部碰撞排除 -->
    <exclude body1="ag95_base" body2="left_inner_knuckle"/>
    <exclude body1="ag95_base" body2="left_outer_knuckle"/>
    <exclude body1="ag95_base" body2="right_inner_knuckle"/>
    <exclude body1="ag95_base" body2="right_outer_knuckle"/>
    <exclude body1="ag95_base" body2="left_finger"/>
    <exclude body1="ag95_base" body2="right_finger"/>
    <exclude body1="left_inner_knuckle" body2="left_finger"/>
    <exclude body1="right_inner_knuckle" body2="right_finger"/>
    
    <!-- 跨系统碰撞排除 -->
    <exclude body1="wrist_3_link" body2="ag95_base"/>
    <exclude body1="wrist_3_link" body2="left_inner_knuckle"/>
    <exclude body1="wrist_3_link" body2="right_inner_knuckle"/>
    <exclude body1="wrist_3_link" body2="left_outer_knuckle"/>
    <exclude body1="wrist_3_link" body2="right_outer_knuckle"/>
  </contact>

  <!-- AG95腱传动系统 -->
  <tendon>
    <fixed name="split">
      <joint joint="left_outer_knuckle_joint" coef="0.5"/>
      <joint joint="right_outer_knuckle_joint" coef="0.5"/>
    </fixed>
  </tendon>

  <!-- AG95等式约束 -->
  <equality>
    <connect anchor="-0.020673 0 0.007524" body1="left_finger" body2="left_inner_knuckle" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <connect anchor="-0.020673 0 0.007524" body1="right_finger" body2="right_inner_knuckle" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <joint joint1="left_outer_knuckle_joint" joint2="right_outer_knuckle_joint" polycoef="0 1 0 0 0" solimp="0.95 0.99 0.001" solref="0.005 1"/>
  </equality>

  <!-- 驱动器配置 -->
  <actuator>
    <!-- UR3E驱动器 -->
    <general class="size2" name="shoulder_pan" joint="shoulder_pan_joint"/>
    <general class="size2" name="shoulder_lift" joint="shoulder_lift_joint"/>
    <general class="size1_limited" name="elbow" joint="elbow_joint"/>
    <general class="size0" name="wrist_1" joint="wrist_1_joint"/>
    <general class="size0" name="wrist_2" joint="wrist_2_joint"/>
    <general class="size0" name="wrist_3" joint="wrist_3_joint"/>
    
    <!-- AG95驱动器 -->
    <general name="fingers_actuator" tendon="split" class="ag95"/>
  </actuator>
</mujoco>